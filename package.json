{
  "name": "cypress-frontend-dashboard",
  "version": "1.0.0",
  "description": "Repository created in the article Web Tests with Cypress",
  "main": "index.js",
  "files": [
    "templates/",
    "index.js"
  ],
  "scripts": {
    "test:account": "npx cypress run --spec cypress/e2e/Account/**/* --browser chrome",
    "test:login": "npx cypress run --spec cypress/e2e/Login/**/* --browser chrome",
    "test:Signout": "npx cypress run --spec cypress/e2e/Signout/**/* --browser chrome",
    "cy:open": "npx cypress open --browser chrome",
    "test": "npm start & cypress run --browser chrome",
    "e2e": "npx cypress run",
    "prettier": "prettier --write ./cypress/**/*.js --config ./.prettierrc",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "cypress",
    "cypress-boilerplate",
    "cypress-template",
    "boilerplate",
    "template"
  ],
  "devDependencies": {
    "@faker-js/faker": "^7.6.0",
    "cypress": "^11.2.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-mochawesome-reporter": "^3.2.3",
    "cypress-multi-reporters": "^1.6.0",
    "cypress-plugin-tab": "^1.0.5",
    "eslint": "^6.1.0",
    "eslint-plugin-cypress": "^2.12.1",
    "faker": "^5.5.3",
    "glob": "^7.1.4",
    "husky": "^8.0.0",
    "mocha-steps": "^1.3.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.2.0",
    "prettier": "2.8.3",
    "rimraf": "^3.0.2"
  },
  "dependencies": {
    "cypress-plugin-api": "^1.2.3",
    "cypress-xpath": "^2.0.1",
    "date-fns": "^2.29.3",
    "reload": "^3.2.0",
    "uuid": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./husky/pre-commit",
      "pre-push": "./husky/pre-push $HUSKY_GIT_STDIN"
    }
  }
}
